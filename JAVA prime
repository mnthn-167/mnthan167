class Prime{
  public static void main(String[] args)
    {
      int N=Integer.parseInt(args[0]);
      int flag=0;
      for(int i=2; i<=N/2; i++)
       {
         if(N%i==0)
        {
           flag=1;

           break;
        }
    }
           if(flag==0)
           System.out.println("it is a prime number");
           else
           System.out.println("it is not a prime number");
}
}
